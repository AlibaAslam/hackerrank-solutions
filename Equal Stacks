#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

int max(int a, int b, int c) {
    if (a >= b && a >= c) return a;
    if (b >= a && b >= c) return b;
    return c;
}

int equalStacks(int h1_size, int* h1, int h2_size, int* h2, int h3_size, int* h3) {
    long long sum1 = 0, sum2 = 0, sum3 = 0;
    int i1 = 0, i2 = 0, i3 = 0;

 
    for (int i = 0; i < h1_size; i++) sum1 += h1[i];
    for (int i = 0; i < h2_size; i++) sum2 += h2[i];
    for (int i = 0; i < h3_size; i++) sum3 += h3[i];

    
    while (!(sum1 == sum2 && sum2 == sum3)) {
    
        if (i1 == h1_size || i2 == h2_size || i3 == h3_size) {
            return 0;
        }

        
        long long current_max = max(sum1, sum2, sum3);

        
        if (sum1 == current_max) {
            sum1 -= h1[i1++];
        } else if (sum2 == current_max) {
            sum2 -= h2[i2++];
        } else { 
            sum3 -= h3[i3++];
        }
    }

    
    return (int)sum1; 
}


int main() {
    int n1, n2, n3;
    if (scanf("%d %d %d", &n1, &n2, &n3) != 3) return 1;

    int* h1 = (int*)malloc(n1 * sizeof(int));
    int* h2 = (int*)malloc(n2 * sizeof(int));
    int* h3 = (int*)malloc(n3 * sizeof(int));

    
    for (int i = 0; i < n1; i++) {
        if (scanf("%d", &h1[i]) != 1) return 1;
    }
   
    for (int i = 0; i < n2; i++) {
        if (scanf("%d", &h2[i]) != 1) return 1;
    }
    
    for (int i = 0; i < n3; i++) {
        if (scanf("%d", &h3[i]) != 1) return 1;
    }

    int result = equalStacks(n1, h1, n2, h2, n3, h3);
    printf("%d\n", result);

    free(h1);
    free(h2);
    free(h3);

    return 0;
}
