#include <stdio.h>
#include <string.h>
#include <stdbool.h>

void solve() {
    int N;
    if (scanf("%d", &N) != 1) return;
    char B[1001];
    if (scanf("%s", B) != 1) return;
    
    int counts[26] = {0};
    bool has_underscore = false;
    int len = strlen(B);
    
    for (int i = 0; i < len; i++) {
        if (B[i] == '_') {
            has_underscore = true;
        } else {
            counts[B[i] - 'A']++;
        }
    }
    
    for (int i = 0; i < 26; i++) {
        if (counts[i] == 1) {
            printf("NO\n");
            return;
        }
    }
    
    if (has_underscore) {
        printf("YES\n");
        return;
    }
    
    for (int i = 0; i < len; i++) {
        char current_bug = B[i];
        bool is_happy = false;
        
        if (i > 0 && B[i-1] == current_bug) {
            is_happy = true;
        }
        
        if (i < len - 1 && B[i+1] == current_bug) {
            is_happy = true;
        }
        
        if (!is_happy) {
            printf("NO\n");
            return;
        }
    }
    
    printf("YES\n");
}

int main() {
    int G;
    if (scanf("%d", &G) != 1) return 1;
    while (G--) {
        solve();
    }
    return 0;
}
