#include <stdio.h>
#include <string.h>

void solve_testcase() {
    int n;
    int k;
    
    if (scanf("%d %d", &n, &k) != 2) {
        return; 
    }

    int counts[100];
    
    for (int i = 0; i < k; i++) {
        counts[i] = 0;
    }
    
    long long result = 0;

    int current_remainder = 0;

    counts[0] = 1;

    for (int i = 0; i < n; i++) {
        int a_i;
        if (scanf("%d", &a_i) != 1) {
            return;
        }

        current_remainder = (current_remainder + a_i) % k;
        
        result += counts[current_remainder];

        counts[current_remainder]++;
    }

    printf("%lld\n", result);
}

int main() {
    
    int t;
    if (scanf("%d", &t) != 1) {
        return 1;
    }
    
    while (t--) {
        solve_testcase();
    }
    
    return 0;
}
